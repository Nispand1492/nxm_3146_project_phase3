<!DOCTYPE html>
<html>
<head>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script>
$(document).ready(function()
{
        $("table").hide();
        $("button#butt").click(function()
        {
                $("#butt").hide();
		setTimeout(function()
		{
			for(i=1;i<=100;i++)
			{
				url = "/result/"+i;
                		$.get(url, function(data, status)
                		{       
                       	 		console.log(data);
                        		var row = $("<tr/>");
                        		$("#tab").append(row);
                        		row.append($("<td>" + data[0].id + "</td>"));
                       			row.append($("<td>" + data[0].label + "</td>"));
                        		row.append($("<td>" + data[0].concept + "</td>"));
                        		row.append($("<td>" + data[0].predicateType + "</td>"));
				
               		 	});
			}},500);
       	$("table").show();
        });
});


</script>
<style>
        .center {
    margin-left: 25%;
    margin-right: 40%;
    margin-top: 20%;
    width: 50%;
    height: 40%;
    -webkit-border-radius: 28;
    -moz-border-radius: 28;
    border-radius: 28px;
    font-family: Arial;
    font-size:30px;
    color: #ffffff;
    background-color: #b0e0e6;
}


        body
        {
                background-image: url("http://derekouellette.ca/wp-content/uploads/2014/07/Open-Book-Macro-Wallpaper-Background-760x406.jpg");
                background-size: 1500px 760px;
                background-repeat: no-repeat;
        }

</style>

</head>
<body>
<div  class="wrapper">
<button  class="center" id="butt">Click here to get display Census Data</button>
<table id="tab" border="1">
        <caption>Census Data 2013</caption>
        <tr>
                <th>ID</th>
                <th>Label</th>
                <th>Concept</th>
                <th>Predicate Type</th>
      </tr>
</table>
</div>

</html>



